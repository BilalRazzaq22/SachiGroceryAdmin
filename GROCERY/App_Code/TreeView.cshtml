@helper GetTreeView(List<GROCERY.Models.MAINMENU> siteMenu, int parentID)
{
    foreach (var i in siteMenu.Where(a => a.MAINMENU1.Equals(parentID)))
    {
        <li>
            @{var submenu = siteMenu.Where(a => a.MAINMENU1.Equals(i.ID)).Count();}
            @if (submenu > 0)
            {
                <span class="collapse collapsible">&nbsp;</span>
            }
            else
            {
                <span style="width:15px; display:inline-block">&nbsp;</span>
            }
            <span>
                <a>@i.MAINMENU1</a>
            </span>
            @if (submenu > 0)
            {
                <ul>
                    @TreeView.GetTreeView(siteMenu, i.ID)
                    @* Recursive  Call for Populate Sub items here*@
                </ul>
            }
        </li>
    }
}