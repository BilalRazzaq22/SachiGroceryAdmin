                            $("#wait").css("display", "none");
                            alert("Stock Sync Complete Successfully");
                        }
                        else {
                            $("#maindiv").removeClass("disable-maindiv");
                            $("#wait").css("display", "none");
                            alert(result);
                        }

                    },
                    error: function (res) {
                        $("#maindiv").removeClass("disable-maindiv");
                        $("#wait").css("display", "none");
                        alert(res);
                    }
                });
            });

            $("#productSync").click(function () {
                $("#maindiv").addClass("disable-maindiv");
